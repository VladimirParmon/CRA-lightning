{
  "name": "PipeLaunch",
  "version": "3.0",
  "description": "A linkedin chrome extension for an easy Salesforce integration",
  "manifest_version": 3,
  "incognito": "split",
  "action": {
    "default_popup": "index.html"
  },
  "background": {
    "service_worker": "static/js/background.2e15b7f6.c01bf175.js",
    "type": "module"
  },
  "web_accessible_resources": [
    {
      "resources": ["index.html"],
      "matches": ["https://www.linkedin.com/*"]
    }
  ],
  "content_scripts": [
    {
      "matches": ["https://www.linkedin.com/*"],
      "js": ["static/js/main.b663dd11.js"]
    }
  ],
  "permissions": ["tabs", "storage", "identity", "scripting", "management", "system.display"],
  "host_permissions": [
    "https://www.linkedin.com/*",
    "https://*.my.salesforce.com/*",
    "https://hwg3ftb7a0.execute-api.eu-central-1.amazonaws.com/*"
  ]
}
